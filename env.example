# Solana RPC Configuration
# SOLANA_RPC_URL=https://api.devnet.solana.com
# SOLANA_WS_URL=wss://api.devnet.solana.com

# Authority Keypair (JSON array format)
# Generate with: solana-keygen new --outfile ~/gbpl-authority.json
# Then convert to JSON array format for this env var
AUTHORITY_PRIVATE_KEY=

# Token Configuration
VITE_GBPL_MINT_ADDRESS=
VITE_USDC_MINT_ADDRESS=

# Vault Configuration
VITE_USDC_VAULT_TOKEN_ACCOUNT=
VITE_GBPL_VAULT_TOKEN_ACCOUNT=

# Upstash Redis (for duplicate prevention)
# Get from https://console.upstash.com/
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Dynamic Labs Configuration
# Get from https://app.dynamic.xyz/
VITE_DYNAMIC_ENVIRONMENT_ID=
